---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>bendoerr.me</title>
 <link href="http://bendoerr.me/atom.xml" rel="self" />
 <link href="http://bendoerr.me/" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://bendoerr.me/</id>
 <author>
   <name>Ben Doerr</name>
   <email>tiggerizzy@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://bendoerr.me/{{ post.url }}" />
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://lebobs.ch/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>